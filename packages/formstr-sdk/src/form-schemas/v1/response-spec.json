{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Form Spec Schema v1",
  "type": "object",
  "properties": {
    "questionId": {
      "type": "string",
      "examples": ["23DSF1", "AA12D3"]
    },
    "answer": {
      "type": "string",
      "examples": ["Correct Answer", "Incorrect answer"]
    },
    "message": {
      "type": "string"
    }
  },
  "required": ["questionId", "answer"]
}
